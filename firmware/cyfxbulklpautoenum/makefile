FX3FWROOT=../..
FX3PFWROOT=../../u3p_firmware

all:compile

include $(FX3FWROOT)/common/fx3_build_config.mak

MODULE = cyfxbulklpautoenum

SOURCE += $(MODULE).c 
SOURCE += cyfxbulklpdscr.c 

C_OBJECT=$(SOURCE:%.c=./%.o)
A_OBJECT=$(SOURCE_ASM:%.S=./%.o)

EXES = $(MODULE).$(EXEEXT)

$(MODULE).$(EXEEXT): $(A_OBJECT) $(C_OBJECT)
	$(LINK)

$(C_OBJECT) : %.o : %.c cyfxbulklpautoenum.h
	$(COMPILE)

$(A_OBJECT) : %.o : %.S
	$(ASSEMBLE)

clean:
	rm -f ./$(MODULE).$(EXEEXT)
	rm -f ./$(MODULE).map
	rm -f ./*.o


compile: $(C_OBJECT) $(A_OBJECT) $(EXES)

